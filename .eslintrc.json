{
   "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
   ],
   "plugins": ["@typescript-eslint", "react", "prettier", "unicorn"],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "sourceType": "module"
   },
   "env": {
      "browser": true,
      "node": true
   },
   "root": true
}
